//To print uppercase, lowercase , numeric and symbols

#include<iostream>
using namespace std;

int main(){
	
	char a;
	cin>>a;
	
	if(a >='a' && a <='z'){
		cout<<"lowercase"<<endl;
	}
	else if(a >='A' && a <='Z'){
		cout<<"Uppercase"<<endl;
	}
	else if(a >= '0' && a <= '9'){
		cout<<"Numeric"<<endl;
	}
	else{
		cout<<"Symbol"<<endl;
	}

return 0;
}
